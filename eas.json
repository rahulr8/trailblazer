{
  "cli": {
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "env": {
        "APP_VARIANT": "development"
      },
      "ios": {
        "resourceClass": "default",
        "buildConfiguration": "Debug"
      }
    },
    "production": {
      "channel": "production",
      "environment": "production",
      "ios": {
        "resourceClass": "default",
        "buildConfiguration": "Release"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {}
    }
  }
}
